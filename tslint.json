{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-no-multiline-js": false,
    "jsx-alignment": false,
    "quotemark": true,
    "jsx-no-lambda":false,
    "ordered-imports": [
      false
    ],
    "jsx-boolean-value": [
      false
    ]
  }
}
